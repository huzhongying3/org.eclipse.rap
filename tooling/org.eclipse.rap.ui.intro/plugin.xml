<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.intro.configExtension">
      <configExtension
            configId="org.eclipse.ui.intro.universalConfig"
            content="intro/overviewExtensionContent.xml">
      </configExtension>
   </extension>
   <extension
         point="org.eclipse.ui.cheatsheets.cheatSheetContent">
      <cheatsheet
            category="org.eclipse.cheatsheets.category.rap"
            contentFile="cheatsheets/target-install.xml"
            id="org.eclipse.rap.target.cheatsheet"
            name="RAP Target Installation">
      </cheatsheet>
      <category
            id="org.eclipse.cheatsheets.category.rap"
            name="RAP">
      </category>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            defaultHandler="org.eclipse.rap.ui.internal.target.InstallRAPTargetHandler"
            id="org.eclipse.rap.ui.intro.installTarget"
            name="Install RAP target platform">
      </command>
   </extension>
   <!--
   <extension
         point="org.eclipse.pde.core.targets">
      <target
            definition="target/rap.target"
            id="org.eclipse.rap.target"
            name="Rich Ajax Platform (with source)">
      </target>
   </extension>
   -->
   <extension
         point="org.eclipse.core.variables.valueVariables">
      <variable
            initializerClass="org.eclipse.rap.ui.internal.target.RAPTargetDestVariableInitializer"
            name="rap_target_dest"
            readOnly="true">
      </variable>
   </extension>
</plugin>
